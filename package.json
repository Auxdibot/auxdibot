    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
