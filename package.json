   "dependencies": {
      "@prisma/client": "^5.14.0",
      "@types/lodash": "^4.17.4",
      "@types/passport": "^1.0.16",
      "@types/passport-discord": "^0.1.13",
      "axios": "^1.7.2",
      "cors": "^2.8.5",
